{
  "name": "registro-y-login-en-vivo",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec babel-node app/index.js",
    "build": "babel app --out-dir dist --ignore 'app/**/*.test.js' && npm run copy-static",
    "copy-static": "xcopy app\\pages dist\\pages /E /I && xcopy app\\public dist\\public /E /I",
    "prod": "cross-env NODE_ENV=production node dist/index.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.22"
  }
}
